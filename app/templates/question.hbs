<div style="border:1px #ccc solid;margin-left:20px;margin-right:20px;box-shadow:1px 1px 1px 0px #ccc;margin-top:10px;padding:20px">
  {{log model}}
  <h4>{{model.title}}</h4>
  <p>By {{model.author}}</p>
  <hr>
  <p>{{model.contents}}</p>
  <p>{{model.notes}}</p>
  {{question-detail question=model}}
</div>

{{new-answer newAnswer="newAnswer" question=model}}
